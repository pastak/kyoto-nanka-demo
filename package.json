{
  "name": "WelcomeToVR",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "yarn run start:api-server",
    "start-dev": "yarn run run-p start:*",
    "start:vr-client": "node -e \"console.log('open browser at http://localhost:8081/vr/\\n\\n');\" && node node_modules/react-native/local-cli/cli.js start",
    "start:api-server": "node server",
    "postinstall": "yarn run build",
    "build": "node node_modules/react-vr/scripts/bundle.js",
    "open": "node -e \"require('xopen')('http://localhost:8081/vr/')\"",
    "devtools": "react-devtools",
    "test": "jest"
  },
  "dependencies": {
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "npm-run-all": "^4.0.2",
    "ovrui": "~1.4.0",
    "querystring": "^0.2.0",
    "react": "~15.4.1",
    "react-native": "~0.42.0",
    "react-vr": "~1.4.0",
    "react-vr-web": "~1.4.0",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "three": "^0.80.1"
  },
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-preset-react-native": "^1.9.1",
    "flow-bin": "^0.52.0",
    "jest": "^19.0.2",
    "react-devtools": "^2.1.3",
    "react-test-renderer": "~15.4.1",
    "xopen": "1.0.0"
  },
  "jest": {
    "preset": "react-vr"
  }
}
